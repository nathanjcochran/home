{
  "env": {
    "CLAUDE_CODE_DISABLE_FEEDBACK_SURVEY": "1",
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "1"
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "Bash(find:*)",
      "Bash(git diff:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git status:*)",
      "Bash(go build:*)",
      "Bash(go doc:*)",
      "Bash(go env:*)",
      "Bash(go fmt:*)",
      "Bash(go generate:*)",
      "Bash(go get:*)",
      "Bash(go install:*)",
      "Bash(go list:*)",
      "Bash(go mod:*)",
      "Bash(go test:*)",
      "Bash(go tool:*)",
      "Bash(go vet:*)",
      "Bash(gopls rename:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(shellcheck:*)",
      "Bash(staticcheck:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:pkg.go.dev)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebSearch"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "default"
  },
  "model": "opus",
  "enabledPlugins": {
    "gopls-lsp@claude-plugins-official": false
  },
  "syntaxHighlightingDisabled": false
}
