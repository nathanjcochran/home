[alias]
    b = branch
    cm = commit -m
    co = checkout
    d = diff
    dc = diff --cached
    lg = log --all --decorate --oneline --graph
    rao = remote add origin
    rpo = remote prune origin
    rso = remote show origin
    rs = restore --staged
    st = status
    su = submodule update
    sur = submodule update --remote
[credential]
    helper = cache
[pull]
	rebase = true
[fetch]
	prune = true
[diff]
	colorMoved = zebra
